<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="1000.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="w18comp1008s2mar27.GUIExamplesViewController">
    <stylesheets>
        <URL value="@guiexamplesview.css" />
    </stylesheets>
   <children>
      <VBox layoutX="229.0" layoutY="71.0" prefHeight="800.0" prefWidth="1000.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="251.0" prefWidth="1000.0" spacing="20.0">
               <children>
                  <VBox spacing="5.0">
                     <children>
                        <Label text="What I want to do this weekend" />
                        <CheckBox fx:id="soccerCheckBox" mnemonicParsing="false" onAction="#updateWeekendTextArea" text="Play Soccer" />
                        <CheckBox fx:id="videoGameCheckBox" mnemonicParsing="false" onAction="#updateWeekendTextArea" text="Play Video Games" />
                        <CheckBox fx:id="eatCheckBox" mnemonicParsing="false" onAction="#updateWeekendTextArea" text="Eat...it's just fun" />
                        <CheckBox fx:id="lasagnaCheckBox" mnemonicParsing="false" onAction="#updateWeekendTextArea" text="Vlad makes us all Lasagna" />
                        <TextArea fx:id="weekendTextArea" prefHeight="200.0" prefWidth="200.0" />
                     </children>
                  </VBox>
                  <VBox prefHeight="251.0" prefWidth="249.0" spacing="10.0">
                     <children>
                        <ComboBox fx:id="videoGameComboBox" onAction="#comboBoxChanged" promptText="Select Best Video Game" />
                        <Label fx:id="videoGameLabel" text="Label" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
